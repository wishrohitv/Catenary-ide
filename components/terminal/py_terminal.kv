<Terminal>:
    ScrollView:
        do_scroll_y: True
        do_scroll_x: True
        TextInput:
            id: console
            text: root.console_text
            font_size: "18sp"
            markup: True
            do_wrap: True
            foreground_color: (1,1,1,1)
            cursor_color: 0,1,0,1
            cursor_width: "4sp"
            # foreground_color: (247/255,9/255,211/255,1)
            multiline: False
            selection_color: 1,0,1,.23
            background_color: 60/255, 48/255, 72/255, 1
            background_normal: ''
            on_text_validate: root._on_text_validate(self)
